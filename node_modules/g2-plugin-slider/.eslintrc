{
  "extends": "airbnb/legacy",
  "rules": {
    "no-console": [
      0
    ],
    "space-before-function-paren": [
      0
    ],
    "comma-dangle": [2, "never"],
    "vars-on-top": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "new-cap": 0,
    "func-names": 0,
    "no-unused-expressions": 0,
    "dot-notation": [2, {"allowKeywords": false}],
    "quote-props": 0
  }
}
